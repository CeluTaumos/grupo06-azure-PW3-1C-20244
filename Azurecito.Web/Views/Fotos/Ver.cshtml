@model List<string>

<h1 class="mt-3">Todas las Fotos</h1>
<div class="container d-flex flex-wrap mt-3">
    @foreach (var fotoUrl in Model)
    {
        <div class="col-md-6 col-lg-4 col-xl-4 mb-3">
            <div class="card" style="width: 18rem;">
                <img class="image-album" src="@fotoUrl" alt="Foto" onclick="openModal('@fotoUrl')">
                <div class="card-body">
                    <h5 class="card-title">Foto</h5>
                </div>
            </div>
        </div>
    }
</div>


<div id="myModal" class="modal">
    <span class="close" onclick="closeModal()">&times;</span>
    <img class="modal-content" id="img01">
    <div id="caption"></div>
</div>